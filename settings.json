{
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "editor.fontFamily": "'FiraCode Nerd Font'",
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.mouseWheelZoom": true,
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "editor.guides.indentation": true,
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "window.enableMenuBarMnemonics": false,
  "telemetry.telemetryLevel": "off",
  "python.experiments.enabled": false,
  "jupyter.experiments.enabled": false,
  "window.title": "üçÅÔ∏è${dirty}${activeEditorShort} üçÅÔ∏è",
  "workbench.colorCustomizations": {
    "[Dracula]": {
      "editorCursor.foreground": "#ff0000",
      "editorLineNumber.activeForeground": "#ff00aa",
      "editorLineNumber.foreground": "#49a1b7cb",
      "terminalCursor.foreground": "#ff0000",
      "selection.background": "#58ffe941",
      "terminal.selectionBackground": "#58ffe941"
    },
    "[Andromeda]": {
      "editorCursor.foreground": "#ff0000",
      "editorLineNumber.activeForeground": "#ff00aa",
      "editorLineNumber.foreground": "#49a1b7cb",
      "terminal.selectionBackground": "#58ffe941"
    },
    "[Catppuccin Mocha]": {
      "editorCursor.foreground": "#ff0000",
      "editorLineNumber.activeForeground": "#ff00aa",
      "editorLineNumber.foreground": "#49a1b7cb",
      "terminal.selectionBackground": "#58ffe941",
      "terminalCursor.foreground": "#9500ff"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "comments": "#ff00bf",
      "strings": "#00ffdd"
    },
    "[Catppuccin Mocha]": {
      "comments": "#ff00bf",
      "strings": "#00ffdd"
    }
  },
  "window.titleSeparator": "  ‚Üî  ",
  "update.mode": "manual",
  "git.autofetch": true,
  "editor.inlineSuggest.enabled": true,
  "extensions.autoCheckUpdates": false,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": 2,
  "terminal.integrated.shellIntegration.enabled": false,
  "extensions.autoUpdate": false,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-18",
      "path": "/usr/lib/jvm/jdk-18"
    },
    {
      "name": "JavaSE-19",
      "path": "/home/linuxbrew/.linuxbrew/Cellar/openjdk/19/libexec",
      "default": true
    }
  ],
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "workbench.sideBar.location": "right"
}
